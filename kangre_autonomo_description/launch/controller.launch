<launch>

<rosparam file="$(find kangre_autonomo_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="kangre_autonomo" args="Rev7_position_controller Rev8_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/kangre_autonomo/joint_states"/>
</node>

</launch>